{
  "name": "voici-gallery",
  "version": "0.1.0",
  "homepage": "https://trungleduc.github.io/voici-gallery",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/material": "^5.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "npm run build:dashboard && react-scripts build && npm run copy:dashboard",
    "build:dev": "npm run build:dashboard:dev && react-scripts build && npm run copy:dashboard",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dashboard": "python script/build.py",
    "build:dashboard:dev": "python script/build.py --debug",
    "copy:dashboard": "cp -r ./voici_build/voici ./build",
    "lint": "npx prettier --write . --ignore-path .gitignore"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  }
}
